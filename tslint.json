{
  "exclude": [
    "dist",
    "node_modules"
  ],
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "member-access": false,
    "object-literal-sort-keys": [
      false
    ],
    "jsx-no-lambda": [
      false
    ],
    "no-shadowed-variable": [
      false
    ]
  }
}